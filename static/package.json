        "@babel/runtime": "^7.9.2",
        "babel-loader": "^8.1.0",
        "css-loader": "^3.4.2",
        "node-sass": "^4.13.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.1.3",
